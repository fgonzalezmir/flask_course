{% extends 'base/base2.html.j2' %}

{% block content %}

    {% from "_macro_form.html.j2" import render_field %}

    <form name="{{ form }}" id="contactForm" method="POST">
        {{ render_field(form.username) }}
        {{ render_field(form.password) }}
        <input type="hidden" name="csrf_token" value="{{ csrf_token() }}"" />

        <input type="submit">
    </form>

{% endblock %}